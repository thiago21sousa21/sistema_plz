    "povoar-sql": "dotenv -e ./.env -- sh -c 'mysql -u $DB_USER -p$DB_PASSWORD -h $DB_HOST -D $DB_NAME < ./src/database/seeds/populate_bicosthe.sql'",
    "reset-db": "npm run criar-banco-sql && npm run povoar-sql"
